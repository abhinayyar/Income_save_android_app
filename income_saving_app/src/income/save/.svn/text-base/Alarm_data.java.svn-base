package income.save;

public class Alarm_data {
	
	int _id;
	String _name;
	
	int _time;
	
	public Alarm_data(String Alert_Name,int time)
	{
		
		this._name=Alert_Name;
		this._time=time;
		
	}
	
	
	
	public int getID()
	{
		return this._id;
	}
	public void SetID(int ID)
	{
		this._id=ID;
	}
	
	public String getAlertName()
	{
		return this._name;
	}
	
	public int getAlertTime()
	{
		return this._time;
	}
	
	
	public void setAlertName(String Alert_Name)
	{
		this._name=Alert_Name;
	}
	public void setAlertTime(int time)
	{
		this._time=time;
	}
	
	

}
